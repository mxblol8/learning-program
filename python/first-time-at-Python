#logic
x, y = 100,50

print(x==y)
print(x!=y)
print(x<=y)
print(x>=y)

#if..else
score = float(input("please insert your score:"))

if score >= 90:
    print("your grade it's A")
elif score >= 80:
    print("your grade it's B")
elif score >= 70:
    print("your grade it's C")
elif score >= 60:
    print("your grade it's D")
else:
    print("your grade it's F")